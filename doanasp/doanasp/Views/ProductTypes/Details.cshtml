@model doanasp.Models.ProductType

@{
    ViewData["Title"] = "Details";
    Layout = "_LayoutAdminIndex";
    string username = "anonymous";
    if (ViewBag.UserName != null)
    {
        username = ViewBag.UserName;
        @section Member{<li><a asp-controller="Accounts" asp-action="Logout">Logout</a></li>}
    }
    else
    {
        @section Member{}


    }
}
@section name{@username}
<div class="container">
    <h1>Details</h1>

    <div>
        <h4>ProductType</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.TypeName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TypeName)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Status)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Status)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
        <a asp-action="Index">Back to List</a>
    </div>
</div>